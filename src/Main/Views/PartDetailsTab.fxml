<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="180.0" prefWidth="438.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.Controllers.PartDetailsTabContentController">
    <children>
        <Label layoutX="24.0" layoutY="30.0" text="Part Name" />
        <TextField fx:id="partNameField" layoutX="126.0" layoutY="26.0" />
        <Button fx:id="partSearchButton" layoutX="294.0" layoutY="26.0" mnemonicParsing="false" onAction="#partSearch_Click" text="Search" />
        <TableView fx:id="partDetailsTable" layoutX="58.0" layoutY="78.0" prefHeight="90.0" prefWidth="322.0">
            <columns>
                <TableColumn editable="false" prefWidth="160.0" text="Property">
                    <cellValueFactory>
                        <PropertyValueFactory property="propertyName" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn editable="false" minWidth="0.0" prefWidth="161.0" text="Value">
                    <cellValueFactory>
                        <PropertyValueFactory property="value" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
            <items>
                <FXCollections fx:factory="observableArrayList">
                    
                </FXCollections>
            </items>
        </TableView>
    </children>
</AnchorPane>
