<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="350.0" prefWidth="438.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.Controllers.FXMLDocumentController">
   <children>
      <TabPane prefHeight="350.0" prefWidth="438.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="newPartTab" text="New Part(s)">
            <content>
                <fx:include fx:id="addPartTabContent" source="AddPartTab.fxml" />
            </content>
          </Tab>
          <Tab fx:id="partDetailsTab" text="Part Details">
            <content>
                <fx:include fx:id="partDetailsTabContent" source="PartDetailsTab.fxml" />
            </content>
          </Tab>
            <Tab fx:id="partsListTab" onSelectionChanged="#partsListTabSelected" text="Parts List">
              <content>
                <fx:include fx:id="partListTabContent" source="PartListTab.fxml" />
              </content>
            </Tab>
            <Tab onSelectionChanged="#sellPartTabSelected" text="Sell Part">
              <content>
                <fx:include fx:id="sellPartTabContent" source="SellPartTab.fxml" />
              </content>
            </Tab>
            <Tab fx:id="transferPartsTab" text="Transfer Parts">
              <content>
                <fx:include fx:id="transferPartsContentTab" source="TransferPartsTab.fxml" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
